{% extends 'global/layout_form.html.twig' %}

{% set titlePage = 'page.clientes.title.edit' | trans %}
{% set menu = 'clientes' %}
{% set pathCancel = path('clientes_index') %}

{% block formContent %}
<div class="znv-half">
    {{ form_row(form.nombre) }}
</div>
<div class="znv-half">
    {{ form_row(form.direccion) }}
</div>
<div class="znv-half">
    {{ form_row(form.telefono) }}
</div>
<div class="znv-half">
    {{ form_row(form.tipoIva) }}
</div>
<div class="znv-half">
    {{ form_row(form.documento) }}
</div>
{% endblock %}